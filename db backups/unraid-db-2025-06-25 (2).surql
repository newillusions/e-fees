DEFINE TABLE fee TYPE NORMAL SCHEMAFULL PERMISSIONS FULL;

DEFINE FIELD activity ON fee TYPE option<string> PERMISSIONS FULL;
DEFINE FIELD company_id ON fee TYPE record<company> ASSERT $value != NONE PERMISSIONS FULL;
DEFINE FIELD contact_id ON fee TYPE record<contacts> ASSERT $value != NONE PERMISSIONS FULL;
DEFINE FIELD issue_date ON fee TYPE string ASSERT $value != NONE PERMISSIONS FULL;
DEFINE FIELD name ON fee TYPE string ASSERT $value != NONE AND string::len($value) > 0 PERMISSIONS FULL;
DEFINE FIELD number ON fee TYPE string ASSERT $value != NONE PERMISSIONS FULL;
DEFINE FIELD package ON fee TYPE option<string> PERMISSIONS FULL;
DEFINE FIELD project_id ON fee TYPE record<projects> ASSERT $value != NONE PERMISSIONS FULL;
DEFINE FIELD rev ON fee TYPE int DEFAULT 1 PERMISSIONS FULL;
DEFINE FIELD revisions ON fee TYPE array<object> DEFAULT [] PERMISSIONS FULL;
DEFINE FIELD revisions[*] ON fee TYPE object PERMISSIONS FULL;
DEFINE FIELD staff_email ON fee TYPE option<string> PERMISSIONS FULL;
DEFINE FIELD staff_name ON fee TYPE option<string> PERMISSIONS FULL;
DEFINE FIELD staff_phone ON fee TYPE option<string> PERMISSIONS FULL;
DEFINE FIELD staff_position ON fee TYPE option<string> PERMISSIONS FULL;
DEFINE FIELD stage ON fee TYPE string DEFAULT 'Draft' ASSERT $value INSIDE ['Draft', 'Prepared', 'Sent', 'Under Review', 'Clarification', 'Negotiation', 'Awarded', 'Lost'] PERMISSIONS FULL;
DEFINE FIELD status ON fee TYPE string DEFAULT 'Draft' ASSERT $value INSIDE ['Draft', 'Active', 'Sent', 'Awarded', 'Lost', 'Cancelled'] PERMISSIONS FULL;
DEFINE FIELD strap_line ON fee TYPE option<string> DEFAULT 'sensory design studio' PERMISSIONS FULL;
DEFINE FIELD time ON fee TYPE object VALUE { created_at: time::now(), updated_at: time::now() } PERMISSIONS FULL;

DEFINE INDEX fee_project_rev ON fee FIELDS project_id, rev UNIQUE;


