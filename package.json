{
  "name": "e-fees",
  "version": "0.10.9",
  "main": "index.js",
  "scripts": {
    "version:sync": "node scripts/sync-version.cjs",
    "version:set": "node scripts/sync-version.cjs",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:dev": "E2E_DEV_MODE=true playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:mcp": "vitest run e2e-mcp/tests/",
    "test:webdriver": "wdio run ./wdio.conf.js",
    "test:webdriver:build": "npm run build && cargo build --release --manifest-path=src-tauri/Cargo.toml",
    "test:e2e:mcp:ui": "vitest --ui e2e-mcp/tests/",
    "test:e2e:mcp:watch": "vitest --watch e2e-mcp/tests/",
    "test:e2e:mcp:coverage": "vitest run --coverage e2e-mcp/tests/",
    "test:e2e:cleanup": "node -e \"import('./e2e-mcp/fixtures/cleanup-utilities.js').then(m => m.cleanupAllTestData().then(r => console.log('Cleanup completed:', r)))\"",
    "test:e2e:list-test-data": "node -e \"import('./e2e-mcp/fixtures/cleanup-utilities.js').then(m => m.listAllTestData().then(r => console.log('Test data found:', r)))\"",
    "test:e2e:verify-clean": "node -e \"import('./e2e-mcp/fixtures/cleanup-utilities.js').then(m => m.verifyCleanup().then(r => console.log('Cleanup verification:', r)))\"",
    "test:e2e:emergency-cleanup": "node -e \"import('./e2e-mcp/fixtures/cleanup-utilities.js').then(m => m.emergencyCleanup().then(r => console.log('Emergency cleanup:', r)))\"",
    "test:e2e:safe": "npm run test:e2e:mcp && npm run test:e2e:cleanup && npm run test:e2e:verify-clean",
    "test:all": "npm run test:run && npm run test:e2e:mcp",
    "perf:install": "cd performance && npm install && ./scripts/setup.sh",
    "perf:test": "cd performance && ./scripts/run-all-tests.sh",
    "perf:test:smoke": "cd performance && ./scripts/run-smoke-tests.sh",
    "perf:test:database": "cd performance && k6 run tests/database/bulk-operations.js",
    "perf:test:ui": "cd performance && playwright test tests/ui/large-lists.spec.js",
    "perf:test:workflows": "cd performance && k6 run tests/workflows/complete-workflows.js",
    "perf:test:memory": "cd performance && node tests/memory/memory-leak-detection.js",
    "perf:test:regression": "cd performance && node tests/regression/performance-regression.js",
    "perf:generate-data": "cd performance && node data/generators/generate-large-dataset.js",
    "perf:baseline": "cd performance && node utils/monitoring/create-baseline.js",
    "perf:compare": "cd performance && node utils/monitoring/compare-performance.js",
    "perf:report": "cd performance && node utils/reporters/generate-html-report.js",
    "perf:monitor": "cd performance && node utils/monitoring/live-performance-monitor.js",
    "perf:clean": "cd performance && ./scripts/cleanup.sh",
    "perf:docker:up": "cd performance && docker-compose -f docker/docker-compose.yml up -d",
    "perf:docker:down": "cd performance && docker-compose -f docker/docker-compose.yml down",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build"
  },
  "type": "module",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "E-Fees - Digital Fee Proposal Management System",
  "devDependencies": {
    "@crabnebula/tauri-driver": "^2.0.8",
    "@sveltejs/vite-plugin-svelte": "^5.1.0",
    "@tauri-apps/cli": "^2.5.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/svelte": "^5.2.8",
    "@testing-library/user-event": "^14.6.1",
    "@tsconfig/svelte": "^5.0.4",
    "@types/node": "^24.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "@wdio/cli": "^9.19.2",
    "@wdio/local-runner": "^9.19.2",
    "@wdio/mocha-framework": "^9.19.2",
    "@wdio/spec-reporter": "^9.19.2",
    "autoprefixer": "^10.4.21",
    "happy-dom": "^18.0.1",
    "jsdom": "^26.1.0",
    "playwright": "^1.54.1",
    "postcss": "^8.5.5",
    "svelte": "^5.34.1",
    "svelte-check": "^4.2.1",
    "tailwindcss": "^3.4.17",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.2.4",
    "webdriverio": "^9.19.2"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.5.0",
    "@tauri-apps/plugin-dialog": "^2.2.2",
    "@tauri-apps/plugin-process": "^2.3.1",
    "@tauri-apps/plugin-updater": "^2.9.0",
    "node-fetch": "^3.3.2",
    "surrealdb": "^1.3.2",
    "surrealdb.js": "^1.0.0",
    "svelte-spa-router": "^4.0.1",
    "tauri-plugin-mcp": "file:tauri-plugin-mcp",
    "ws": "^8.18.2"
  }
}
